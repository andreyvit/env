<?xml version="1.0"?>
<root>
  <item>
      <name>F17 to Control + Shift + Option</name>
      <appendix>Change F17 (remapped Control_L via Seil) to Control + Shift + Option.</appendix>
 
      <identifier>com.tarantsov.f17_to_front</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::F17,
 
          KeyCode::CONTROL_L,
          ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L
      </autogen>
  </item>
  <item>
      <name>PC Insert to ยง</name>
      <appendix>Change PC Keyboard Insert key to ยง.</appendix>
 
      <identifier>com.tarantsov.pc_insert_to_danish_dollar</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::HELP,
 
          KeyCode::DANISH_DOLLAR
      </autogen>
  </item>
  <item>
      <name>PC Num Lock to Fn</name>
      <appendix>Change PC Num Lock key to Fn.</appendix>
 
      <identifier>com.tarantsov.numlock_to_fn</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::KEYPAD_CLEAR,
 
          KeyCode::FN
      </autogen>
  </item>
</root>
