<?xml version="1.0"?>
<root>
  <item>
      <name>F17 to Control + Shift + Option</name>
      <appendix>Change F17 (remapped Control_L via Seil) to Control + Shift + Option.</appendix>
 
      <identifier>com.tarantsov.f17_to_front</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::F17,
 
          KeyCode::CONTROL_L,
          ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L
      </autogen>
  </item>
  <item>
      <name>PC Insert to ยง</name>
      <appendix>Change PC Keyboard Insert key to ยง.</appendix>
 
      <identifier>com.tarantsov.pc_insert_to_danish_dollar</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::HELP,
 
          KeyCode::DANISH_DOLLAR
      </autogen>
  </item>
  <item>
      <name>F13 to Fn</name>
      <appendix></appendix>
 
      <identifier>com.tarantsov.f13_to_fn</identifier>
 
      <autogen>
          --KeyToKey--
          KeyCode::F13,
           KeyCode::FN
      </autogen>
  </item>
  <item>
      <name>Num Lock to Command-W in web browsers</name>
      <appendix>Num Lock to Close Window in Safari and Chrome</appendix>
      <identifier>com.tarantsov.numlock.command_w_in_browser</identifier>
      <only>SAFARI, GOOGLE_CHROME</only>

      <autogen>
          --KeyToKey--
          KeyCode::KEYPAD_CLEAR,
          KeyCode::W, ModifierFlag::COMMAND_L
      </autogen>
  </item>
</root>
